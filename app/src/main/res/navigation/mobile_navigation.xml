<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_calendar">
    <fragment
        android:id="@+id/navigation_calendar"
        android:name="app.igarashi.igaryo.recruitingapp.fragment.CalendarFragment"
        tools:layout="@layout/fragment_calendar"
        android:label="@string/title_calendar"/>

    <fragment
        android:id="@+id/navigation_search"
        android:name="app.igarashi.igaryo.recruitingapp.fragment.SearchFragment"
        tools:layout="@layout/fragment_search"
        android:label="@string/title_search"/>

    <fragment
        android:id="@+id/navigation_setting"
        android:name="app.igarashi.igaryo.recruitingapp.fragment.SettingFragment"
        tools:layout="@layout/fragment_setting"
        android:label="@string/title_setting"/>

</navigation>